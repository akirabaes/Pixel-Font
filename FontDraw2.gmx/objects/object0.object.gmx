/*for(cc=1; cc&lt;=string_length(characters); cc+=1)
    {draw_sprite(1,cc-1,cc*4+1,1);}
*/

ww=0

w=2
h=4
x0=1
y0=1
x=x0
y=y0
for(letterpos=1; letterpos&lt;=string_length(mystring); letterpos+=1){
    letter = string_char_at(mystring,letterpos)
    letnum = string_pos(letter,characters)-1
    
    if(letter == "#"){
        x=x0
        y+=h+vspacing
    }
    else if letter == " "{
        x+=w+hspacing
        ww=max(ww,x)
    }
    else if letnum != -1
    {
        draw_sprite(1,letnum,x,y)
        x+=w+hspacing
        ww=max(ww,x)
    }
    else
    {
        draw_sprite(1,28,x,y)
        x+=w+hspacing
        ww=max(ww,x)
    }
}

ww=ww-hspacing+1
hh=y+h+1
</string>
          </argument>
        </arguments>
      </action>
    </event>
